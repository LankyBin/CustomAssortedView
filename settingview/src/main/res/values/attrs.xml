<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--    Common-->
    <attr name="icon" format="reference"/>
    <attr name="name" format="string|reference"/>
    <attr name="name_color" format="reference"/>

    <declare-styleable name="DefaultItemLayout">
        <attr name="icon"/>
        <attr name="name"/>
        <attr name="name_color"/>
    </declare-styleable>

    <declare-styleable name="SelectItemLayout">
        <attr name="icon"/>
        <attr name="name"/>
        <attr name="name_color"/>
        <attr name="options" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SwitchItemLayout">
        <attr name="icon"/>
        <attr name="name"/>
        <attr name="name_color"/>
    </declare-styleable>

    <declare-styleable name="SeekbarItemLayout">
        <attr name="icon"/>
        <attr name="name"/>
        <attr name="name_color"/>
        <attr name="seekbar_progress_min" format="integer"/>
        <attr name="seekbar_progress_max" format="integer"/>
        <attr name="show_value" format="boolean"/>
    </declare-styleable>
</resources>